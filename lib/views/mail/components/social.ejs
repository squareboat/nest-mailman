<mj-section background-color="#fbfbfb">
  <mj-column>
    <mj-social font-size="15px" icon-size="30px" mode="horizontal">
      <% for(let app of social) { %>
      <mj-social-element name="<%=app.name%>" href="<%=app.link%>">
        <%=app.label%>
      </mj-social-element>
      <% } %>
    </mj-social>
  </mj-column>
</mj-section>
